var mainWindow=Ti.UI.createWindow({backgroundColor:"black",navBarHidden:!0});if(console.log(Ti.Platform.osname),"iphone"===Ti.Platform.osname||"ipad"===Ti.Platform.osname)var navigationWindow=Titanium.UI.iOS.createNavigationWindow({window:mainWindow});exports.buildUI=function(e){console.log("obj "+JSON.stringify(e));var t=Ti.UI.createScrollableView({width:"100%",height:"60%",top:"19%",backgroundColor:"black",showHorizontalScrollIndicator:!0}),a=[];for(i=0,j=e.length;j>i;i++){var o=Ti.UI.createView({layout:"vertical"}),n=Ti.UI.createImageView({image:e[i].image,top:0,width:Ti.UI.FILL}),r=Ti.UI.createLabel({font:{fontSize:"10dp"},bottom:"10%",color:"white",text:"post by: "+e[i].username});o.add(n),o.add(r),a.push(o),t.views=a}var d=0;setInterval(function(){d>=t.views.length&&(d=0),t.scrollToView(d),d++},3e3),mainWindow.add(t),mainWindow.open()};var hyp3Img=Ti.UI.createImageView({image:"1669679_523030604478031_865814781_o.jpg",layout:"vertical",top:0}),tagLabel=Ti.UI.createLabel({text:"#HYP3Life",textAlignment:"center",color:"white",fontSize:"25dp",top:"15%"}),igButton=Ti.UI.createView({backgroundColor:"#3f729b",width:"33%",height:"10%",bottom:"10.3%",left:0}),igView=Ti.UI.createImageView({image:"icons/icons_09.png",height:"50%"}),igLabel=Ti.UI.createLabel({textAlignment:"center",color:"white",font:{fontSize:"15dp",fontFamily:"Helvetica"}});"android"==Ti.Platform.osname&&(igLabel.text="instagram");var igWindow=Ti.UI.createWindow({backgroundColor:"pink",title:"#HYP3Life"});igButton.addEventListener("click",function(){var e=Ti.UI.createWindow(),t=Titanium.UI.createWebView({url:"http://instagram.com/hyp3orlando"});e.add(t),"iphone"===Ti.Platform.osname||"ipad"===Ti.Platform.osname?(navigationWindow.open(),navigationWindow.openWindow(e)):e.open()});var fbButton=Ti.UI.createView({backgroundColor:"#3b5998",width:"33%",height:"10%",bottom:"10.3%"}),fbLabel=Ti.UI.createLabel({textAlignment:"center",color:"white",font:{fontSize:"15dp",fontFamily:"Helvetica"}});"android"==Ti.Plat